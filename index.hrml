    <script>
        document.addEventListener('DOMContentLoaded', function () {
            const skillsData = {
                labels: ['Python', 'SQL', 'AWS', 'Looker (LookML)', 'Power BI', 'QuickSight', 'Git', 'Kestra', 'Análise (K-Means/RFM)'],
                datasets: [{
                    label: 'Proficiência',
                    data: [90, 95, 80, 90, 90, 85, 90, 70, 85],
                    backgroundColor: [
                        'rgba(236, 72, 153, 0.7)', /* Pink 500 */
                        'rgba(236, 72, 153, 0.7)', /* Pink 500 */
                        'rgba(192, 132, 252, 0.7)', /* Purple 400 */
                        'rgba(251, 113, 133, 0.7)', /* Rose 400 */
                        'rgba(251, 113, 133, 0.7)', /* Rose 400 */
                        'rgba(251, 113, 133, 0.7)', /* Rose 400 */
                        'rgba(244, 114, 182, 0.7)', /* Pink 400 */
                        'rgba(168, 85, 247, 0.7)', /* Purple 500 */
                        'rgba(236, 72, 153, 0.7)' /* Pink 500 */
                    ],
                    borderColor: [
                        'rgba(236, 72, 153, 1)', /* Pink 500 */
                        'rgba(236, 72, 153, 1)', /* Pink 500 */
                        'rgba(192, 132, 252, 1)', /* Purple 400 */
                        'rgba(251, 113, 133, 1)', /* Rose 400 */
                        'rgba(251, 113, 133, 1)', /* Rose 400 */
                        'rgba(251, 113, 133, 1)', /* Rose 400 */
                        'rgba(244, 114, 182, 1)', /* Pink 400 */
                        'rgba(168, 85, 247, 1)', /* Purple 500 */
                        'rgba(236, 72, 153, 1)' /* Pink 500 */
                    ],
                    borderWidth: 1,
                    category: ['Linguagens', 'Linguagens', 'Cloud', 'BI & Visualização', 'BI & Visualização', 'BI & Visualização', 'Versionamento', 'Orquestração', 'Análise de Dados']
                }]
            };

            const ctx = document.getElementById('skillsChart').getContext('2d');
            const skillsChart = new Chart(ctx, {
                type: 'bar',
                data: skillsData,
                options: {
                    indexAxis: 'y',
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            display: false
                        },
                        tooltip: {
                            callbacks: {
                                label: function(context) {
                                    let label = context.dataset.label || '';
                                    if (label) {
                                        label += ': ';
                                    }
                                    if (context.parsed.x !== null) {
                                        label += context.parsed.x + '%';
                                    }
                                    return label;
                                },
                                afterLabel: function(context) {
                                    const category = context.dataset.category[context.dataIndex];
                                    return 'Categoria: ' + category;
                                }
                            }
                        }
                    },
                    scales: {
                        x: {
                            beginAtZero: true,
                            max: 100,
                            ticks: {
                                callback: function(value) {
                                    return value + '%'
                                }
                            }
                        },
                        y: {
                           ticks: {
                                autoSkip: false,
                                callback: function(value, index, values) {
                                    const label = this.getLabelForValue(value);
                                    if (label.length > 16) {
                                        return label.substring(0, 16) + '...';
                                    }
                                    return label;
                                }
                            }
                        }
                    }
                }
            });

            const timelineItems = document.querySelectorAll('[data-target]');
            timelineItems.forEach(item => {
                item.addEventListener('click', () => {
                    const targetId = item.getAttribute('data-target');
                    const targetElement = document.getElementById(targetId);
                    
                    document.querySelectorAll('.timeline-item-content.open').forEach(openItem => {
                        if (openItem !== targetElement) {
                            openItem.classList.remove('open');
                        }
                    });

                    targetElement.classList.toggle('open');
                });
            });

            const mobileMenuButton = document.getElementById('mobile-menu-button');
            const mobileMenu = document.getElementById('mobile-menu');
            mobileMenuButton.addEventListener('click', () => {
                mobileMenu.classList.toggle('hidden');
            });
            document.querySelectorAll('#mobile-menu a').forEach(link => {
                link.addEventListener('click', () => {
                    mobileMenu.classList.add('hidden');
                });
            });
        });
